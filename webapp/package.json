{
  "name": "yzzx-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "swa start http://localhost:5173 --api-location ./api --run \"npm run dev:client\"",
    "dev:client": "vite",
    "build:api": "cd api && npm install && npm run build",
    "build": "vite build && npm run build:api",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.vue",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^10.0.0",
    "bootstrap": "^5.3.8",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.20.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.2.0",
    "vue-tsc": "^3.2.1"
  }
}
